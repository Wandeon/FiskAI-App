{
  "name": "fiskai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "tsx scripts/build-search-index.ts",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "worker:sentinel": "npx tsx src/lib/regulatory-truth/workers/sentinel.worker.ts",
    "worker:extractor": "npx tsx src/lib/regulatory-truth/workers/extractor.worker.ts",
    "worker:composer": "npx tsx src/lib/regulatory-truth/workers/composer.worker.ts",
    "worker:reviewer": "npx tsx src/lib/regulatory-truth/workers/reviewer.worker.ts",
    "worker:arbiter": "npx tsx src/lib/regulatory-truth/workers/arbiter.worker.ts",
    "worker:releaser": "npx tsx src/lib/regulatory-truth/workers/releaser.worker.ts",
    "worker:orchestrator": "npx tsx src/lib/regulatory-truth/workers/orchestrator.worker.ts",
    "worker:scheduler": "npx tsx src/lib/regulatory-truth/workers/scheduler.service.ts",
    "bull-board": "npx tsx src/lib/regulatory-truth/workers/bull-board.server.ts",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:unit": "vitest run --exclude \"**/*.db.test.ts\"",
    "test:db": "vitest run --config vitest.config.db.ts",
    "test:watch": "vitest",
    "test:property": "vitest run src/domain/shared/__tests__/Money.property.test.ts src/domain/tax/__tests__/VatBreakdown.property.test.ts src/domain/tax/__tests__/VatCalculator.property.test.ts src/domain/invoicing/__tests__/InvoiceStatus.property.test.ts",
    "test:golden": "vitest run src/lib/fiscal/__tests__/xml-builder.golden.test.ts src/lib/reports/__tests__/pdv-xml.golden.test.ts",
    "test:e2e": "playwright test",
    "test:tenant": "node --import tsx --test src/lib/__tests__/tenant-isolation.test.ts",
    "test:knowledge-hub": "node --import tsx --test src/lib/knowledge-hub/__tests__/*.test.ts",
    "audit:marketing": "tsx scripts/marketing-content-audit/run-audit.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check-features": "tsx scripts/check-feature-tables.ts",
    "prisma:generate": "npm run prisma:generate:core && npm run prisma:generate:regulatory",
    "prisma:generate:core": "prisma generate --schema=prisma/schema.prisma",
    "prisma:generate:regulatory": "prisma generate --config=prisma.config.regulatory.ts",
    "prisma:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:push:regulatory": "prisma db push --config=prisma.config.regulatory.ts",
    "prisma:migrate": "prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:migrate:regulatory": "prisma migrate deploy --config=prisma.config.regulatory.ts",
    "prisma:migrate:dev": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:migrate:dev:regulatory": "prisma migrate dev --config=prisma.config.regulatory.ts",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "prisma:studio:regulatory": "prisma studio --config=prisma.config.regulatory.ts",
    "db:seed": "prisma db seed",
    "db:seed-news": "tsx scripts/seed-news-sources.ts",
    "db:seed-news-categories": "tsx scripts/seed-news-categories.ts",
    "db:seed-deadlines": "tsx scripts/seed-deadlines.ts",
    "db:rewrite-news": "tsx scripts/rewrite-news-posts.ts",
    "build:workers": "tsc -p tsconfig.workers.json && tsc-alias -p tsconfig.workers.json && cp -r src/generated dist/workers/",
    "workers:start": "docker compose -f docker-compose.workers.yml up -d",
    "workers:stop": "docker compose -f docker-compose.workers.yml down",
    "workers:logs": "docker compose -f docker-compose.workers.yml logs -f",
    "workers:status": "docker compose -f docker-compose.workers.yml ps",
    "postinstall": "npm run prisma:generate",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate:routes": "tsx scripts/validate-routes.ts",
    "validate:content": "tsx scripts/validate-content.ts",
    "registry:check": "npx tsx src/lib/system-registry/scripts/registry-check.ts",
    "ci:quick": "npm run prisma:generate && npx tsc --noEmit && NODE_OPTIONS='--max-old-space-size=8192' npx eslint src --ext .ts,.tsx --max-warnings=-1 && npm run registry:check",
    "ci:full": "npm run ci:quick && npm run build && npm test",
    "registry:harvest": "npx tsx src/lib/system-registry/harvesters/index.ts",
    "verify-contrast": "tsx src/design-system/scripts/verify-contrast.ts",
    "test:node": "node --import tsx --test --test-name-pattern='.*' src/lib/__tests__/e-invoice-status.test.ts src/lib/__tests__/joppd-generator.test.ts src/lib/__tests__/login-token.test.ts src/lib/__tests__/marketing-audit-*.test.ts src/lib/__tests__/r2-tenant-isolation.test.ts src/lib/__tests__/rate-limit-fail-closed.test.ts src/lib/__tests__/rbac.test.ts src/lib/__tests__/reports.test.ts src/lib/__tests__/system-registry-blast-score.test.ts src/lib/__tests__/system-registry-enforcement.test.ts src/lib/__tests__/system-registry-github-check.test.ts src/lib/__tests__/system-registry-harvester-errors.test.ts src/lib/__tests__/system-registry-pr-comment.test.ts src/lib/__tests__/system-registry-queues.test.ts src/lib/__tests__/system-registry-workers.test.ts src/lib/__tests__/system-status-*.test.ts src/lib/__tests__/tenant-isolation.test.ts src/lib/e-invoice/__tests__/*.test.ts"
  },
  "lint-staged": {
    "src/app/(app)/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: error' --max-warnings 0"
    ],
    "src/app/(admin)/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: error' --max-warnings 0"
    ],
    "src/app/(staff)/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: error' --max-warnings 0"
    ],
    "src/components/marketing/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: warn'"
    ],
    "src/components/!(marketing)/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: error' --max-warnings 0"
    ],
    "src/app/(marketing)/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --rule 'fisk-design-system/no-hardcoded-colors: warn'"
    ],
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
